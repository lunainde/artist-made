<!DOCTYPE html>
<html>
  <head>
    <title>ArtistMade</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload', defer: true %>
    <!-- app/views/layouts/application.html.erb -->
    <%= favicon_link_tag "favicon.ico", rel: "shortcut icon", type: "image/x-icon" %>
    <!-- <link href="/assets/favicon.ico" rel="shortcut icon" type="image/x-icon" /> -->
    <!-- <link href="/assets/mb-icon.png" rel="apple-touch-icon" type="image/png" /> -->
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
    <link rel="icon" href="/favicon.ico" type="image/x-icon">
  </head>

  <body>
    <%= render 'shared/navbar' unless action_name == "home" %>
    <div class="page-container">
      <%= render 'shared/flashes' %>
      <%= yield %>
    </div>
    <%=
    #render 'shared/footer' unless action_name == "index" && ["artists", "arts"].include?(controller_name)
     %>
  </body>
</html>
